{
  "expo": {
    "name": "wdiodemoapp",
    "slug": "wdiodemoapp",
    "version": "1.0.7",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./src/assets/webdriverio.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/webdriverio.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.wdiodemoapp",
      "buildNumber": "17",
      "infoPlist": {
        "NSFaceIDUsageDescription": "wdiodemoapp requires FaceID access to allows you quick and secure access.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSAllowsLocalNetworking": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/webdriverio.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wdiodemoapp",
      "versionCode": 18,
      "permissions": [
        "USE_BIOMETRIC",
        "USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./src/assets/webdriverio.png"
    },
    "scheme": "wdio",
    "plugins": [
      "expo-local-authentication",
      "expo-splash-screen",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}

